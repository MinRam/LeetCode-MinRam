{
    "title": "LeetCode Learning Notes",
    "author": "MinRam",
    "description": "Used for LeetCode Learning Notes",
    "language": "en",
    "links": {
    },
    "styles": {
        "ebook": "styles/marxico.css"
    },
    "plugins": [
        "anchor-navigation-ex",
        "atoc",
        "back-to-top-button",
        "code",
        "comment",
        "expandable-chapters-small",
        "graph",    
        "highlight",
        "insert-logo",
        "katex",
        "mermaid",
        "-share",
        "sharing-plus",
        "splitter",
        "tbfed-pagefooter"
    ],
    "pluginsConfig": {
        "anchor-navigation-ex": {
            "isShowTocTitleIcon": true,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        },
        "atoc": {
            "addClass": true,
            "className": "atoc"
        },
        "insert-logo": {
            "url": "/Reference/images/logo.png",
            "style": "background: none; max-height: 40px; min-height: 40px;"
        },
        "sharing-plus": {
            "qq": true,
            "google": true,
            "qzone": true
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy MinRam 2020",
            "modify_label": "Update on",
            "modify_format": "YYYY-MM-DD HH:mm"
        }
    }
}